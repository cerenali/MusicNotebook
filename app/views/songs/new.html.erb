  <h1>Songs#new</h1>

  <%= form_for(@song) do |f| %>
  	<div class="field">
  		<%= f.label :title %>
  		<%= f.text_field :title %>
  	</div>
  	<div class="field">
  		<%= f.label :artist %>
  		<%= f.text_field :artist %>
  	</div>
    <div class="field">
      <%= f.label :tags %>
      <%= f.text_field :tag_list %>
    </div>
  	<div class="actions">
  		<%= f.submit %>
  	</div>
  <% end %>